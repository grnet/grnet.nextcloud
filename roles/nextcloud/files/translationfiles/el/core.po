msgstr "Έλεγχος ενημερώσεων εφαρμογών"

#: /app/core/ajax/update.php:175
#, php-format
msgid "Update app \"%s\" from appstore"
msgstr "Ενημέρωση εφαρμογής \"%s\" από το appstore"

#: /app/core/ajax/update.php:181
